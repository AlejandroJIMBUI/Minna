{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/logout/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/signup/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/posts/create/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/profile/delete-avatar/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/profile/update/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/upload-avatar/?$",
			"dest": "_render"
		},
		{
			"src": "^/create-post/?$",
			"dest": "_render"
		},
		{
			"src": "^/dashboard/?$",
			"dest": "_render"
		},
		{
			"src": "^/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/profile/?$",
			"dest": "_render"
		},
		{
			"src": "^/register/?$",
			"dest": "_render"
		},
		{
			"src": "^/search/?$",
			"dest": "_render"
		},
		{
			"src": "^/users(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}